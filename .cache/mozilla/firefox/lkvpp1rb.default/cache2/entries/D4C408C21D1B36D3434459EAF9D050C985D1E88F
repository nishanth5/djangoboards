(function(j$) {
	
	var campaign_id = j$("#jCampaignId").text();
	var site_slug = j$(".jSiteSlug").val();
	
	j$(".jPagePermission").click(TVAPP.openWindow);
	
	
	j$(document).off('click', '.jFacetCollapse').on('click', ".jFacetCollapse", function(){
		var holder = j$(this).parents(".filter_hldr");
		
		if(j$(holder).hasClass("jCollapsedHolder")){
			j$(holder).removeClass("jCollapsedHolder");
		}
		else {
			j$(holder).addClass("jCollapsedHolder");
		}
		
	});
	
	/*
	var campaignRemoveAsset = function(btn,assetType){
		var num_jobs = parseInt(j$(".jNumJobs").text());
		
		if(num_jobs > 1){
			var lozenge = j$(btn).parent(".jLozenge");

			var jobId = j$(lozenge).attr("id").substr(9);

			j$("#campaign_add_"+jobId).attr("checked",false);

			j$.ajax({
				type: 'POST',
				url: TVAPP.guid('/ajax/campaigns/' + j$("#jCampaignId").text() + '/' + assetType + '/' + jobId + '/remove'),
				success: function(result){
						j$(lozenge).remove();
						j$(".jNumJobs").text(num_jobs - 1);
					},
				dataType: 'json',
