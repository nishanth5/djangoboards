	var assetAdd = function(btn,assetType){
		
		var action = "remove";
		if(j$(btn).is(':checked')) {
			action = "add";
		}
		
		var noCampId = false;
		if(j$("#jCampaignId").text() == "") { noCampId = true; }
		var jobId = j$(btn).val();
		var jobName = j$(btn).attr("name");
		
		var campData;
		
		j$.ajax({
			type: 'POST',
			url: TVAPP.guid('/ajax/campaigns/' + j$("#jCampaignId").text() + '/' + assetType + '/' + jobId + '/' + action),
			success: function(result){
				if (noCampId) {
					j$("#jCampaignId").text(result.UserMessage);
					//j$("#campaign_summary_hldr").show();
				}
				if (action == "add") {
					/* LOZENGES - No Longer Used
					var newLozenge = j$("<span class='selection jLozenge'></span>").html('<span class="label">'+ jobName +'</span>').attr("id","jCampLoz_"+jobId);
					
					j$("<span class='x jRemoveLozenge' data-type='" + assetType + "'></span>").click(function(){campaignRemoveAsset(this,assetType);}).appendTo(newLozenge);
					
					j$(newLozenge).appendTo(j$(".jFacetHolder"));
					*/
					
					assetType == 'jobs' ? j$('.jNumJobs').text(parseInt(j$('.jNumJobs').text()) + 1) : j$('.jNumPages').text(parseInt(j$('.jNumPages').text()) + 1);
					updateSummaryStatement();
				} else if (action == "remove") {
					/* LOZENGES - No Longer Used
					j$("#jCampLoz_"+jobId,".jFacetHolder").remove();
					*/
					
					assetType == 'jobs' ? j$('.jNumJobs').text(parseInt(j$('.jNumJobs').text()) - 1) : j$('.jNumPages').text(parseInt(j$('.jNumPages').text()) - 1);
					updateSummaryStatement();
				}

			},
			dataType: 'json',
			error: function(xhr, textStatus, error) { 
				TVAPP.masterErrorHandler(xhr, textStatus, error, null);
			}
		});
		
	};
	
	j$('#jResultsArea').on('click', '.jCampaignAdd', function() {
		assetAdd(this, 'jobs');
	});
	
	j$('.jCampaignPageAdd').click(function() {
		assetAdd(this, 'pages');
	}).removeAttr('disabled');
	
	/* LOZENGES - No Longer Used
	j$(document).on('click','.jRemoveLozengeRecip',function(event){campaignRemoveRecip(event,this);});
	
	var campaignRemoveRecip = function(event,btn){
		event.preventDefault();
		
		var lozenge = j$(btn).parent(".jLozenge");
		
		
		j$(document).trigger("prepareRecipients");
		
		j$.ajax({
			type: 'POST',
			url: TVAPP.guid(j$(btn).attr("href")),
			success: function(result){
				j$(".jNumRecipients").text(result.UserMessage);
				j$(".jNumRecipientsPhrase").text(TVAPP.none_one_many(result.UserMessage,"No recipients","1 recipient","##number## recipients"));
				j$(document).trigger("updateRecipientFacets");
				j$(lozenge).remove();
				if(!j$(".jLozenge",".jFacetHolderRecip").length){
					j$(".jFacetHolderRecip").append(j$('<span id="jCampLozRecip_all" class="selection all_lozenge jLozenge"><span class="label">All Facets</span></span>'));
				}
			},
			dataType: 'json',
			error: function(xhr, textStatus, error) { 
				TVAPP.masterErrorHandler(xhr, textStatus, error, null);
			}
		});
	};
	*/
	
	/*~-~-~-~-~-~-~-~-~ START Save Draft Campaigns ~-~-~-~-~-~-~-~-~*/
	// ----  Save as draft code ----

	var draftSuccess = function(result,dialogObj){
		j$(".jCloseModal",dialogObj).trigger("click");
		
		j$("#jSaveDraft").hide();
		j$("#jCancelBtn").hide();
		j$("#jSaveExit").show();
		
		j$("#jCampaignName").text(j$("input[name='Campaign.name']",dialogObj).val());
		j$("#jCampaignNameHldr").show();
		
		if(j$("#jCampTextArea").length){
			j$("#jCampTextArea").trigger("blur");
		}
		if(j$("#jCampMsgInput").length){
			j$("#jCampMsgInput").trigger("blur");
		}
		
		if (j$('.jMsgEdit').length && msg_globals.contents.body_html) {
			j$(document).trigger('camp_msg_save_editor');
		} else {
			j$('body').modal({ 'text': TVAPP.smDict('Campaign.successfully_saved') });
		}
		
		j$(document).trigger("SaveDraftSuccess");
	};

	var handleSaveDraftInitialize = function(e, data) {
		data = data || {};

		$LAB.script("/campaign/dialog.js?v="+TVAPP.property.version_num).wait(function(){
			j$(".jModalSaveDraft").dialog({
				"campaignId": campaign_id,
				"successFunc": draftSuccess,
				"validationFields": {
					"Campaign.name" : "required"
				}
			});
		});

	};

	j$(document).bind("campaign_save_draft_init", handleSaveDraftInitialize);


	j$("#jSaveDraft").click(function(){


		TVAPP.modal.open(
			"campaign_save_draft",
			{"campaign_id":campaign_id},
			{context:"Campaign.id="+campaign_id}
		);
	});
	/*~-~-~-~-~-~-~-~-~- END Save Draft Campaigns -~-~-~-~-~-~-~-~-~*/
	
	if (j$('.jMsgEdit').length && msg_globals.use_composer_flg == 10) {
		j$('#jSaveExit').click(function(e) {
			e.preventDefault();
			
			j$(document).trigger('camp_msg_save_editor');
		});
	}
	
	/*~-~-~-~-~-~-~-~-~ START Manage Campaigns ~-~-~-~-~-~-~-~-~*/
	// ----  Manage code ----

	var manageSuccess = function(result,dialogObj){
		j$(".jCloseModal",dialogObj).trigger("click");
		j$("#jCampaignName").text(j$("input[name='Campaign.name']",dialogObj).val());
		j$(document).trigger("SaveDraftSuccess");
	};
	
	var nameSchedulerSuccess = function(result,dialogObj){
		j$.log("mistress");
		j$("#jCampaignName").text(j$("input[name='Campaign.name']",dialogObj).val());
		j$(document).trigger("CreateSchedule");
	};

	var bindManageBtns = function(dialogObj) {
		j$("#jManageSubmitDelete",dialogObj).click(function(){
			j$("#jSaveDraft").show();
			j$("#jScheduleStatus").fadeOut(1000,function(){j$(this).removeClass("stat_inactive stat_active").addClass("stat_absent");});
			j$("#jSchedBtnHldr").addClass("schedule_absent");
			window.location = site_slug + "/campaigns/list";
		});

	};

	var handleCampManageInitialize = function(e, data) {
		data = data || {};
		
		var nameSuccess = manageSuccess;
		
		j$.log("Time after time: " + data.scheduler);
		
		if(data.scheduler) {
			j$.log("yeah");
			nameSuccess = nameSchedulerSuccess;
		}
		
		$LAB.script("/campaign/dialog.js?v="+TVAPP.property.version_num).wait(function(){
			j$(".jModalCampaignManage").dialog({
				"campaignId": campaign_id,
				"successFunc": nameSuccess,
				"validationFields": {
					"Campaign.name" : "required"
				}
			});
		});

	};

	j$(document).bind("campaign_manage_init", handleCampManageInitialize);
	
	j$(document).on('click','.jManageCampaign',function(){

		TVAPP.modal.open(
			"campaign_manage",
			{"campaign_id":campaign_id},
			{context:"Campaign.id="+campaign_id}
		);

	});
	/*~-~-~-~-~-~-~-~-~- END Manage Campaigns -~-~-~-~-~-~-~-~-~*/
	
	
	/*~-~-~-~-~-~-~-~-~ START Delete Campaigns ~-~-~-~-~-~-~-~-~*/
	
	var handleCampDeleteInitialize = function(e, data) {
		data = data || {};
		
		$LAB.script("/campaign/dialog.js?v="+TVAPP.property.version_num).wait(function(){
			j$(".jModalCampaignDelete").dialog({
				"campaignId": campaign_id,
				"successFunc": manageSuccess,
				"customFunc": bindManageBtns
			});
		});
		
	};
	
	j$(document).bind("campaign_delete_init", handleCampDeleteInitialize);
	
	j$(document).on('click','.jDeleteCampaign',function(){
		//TVAPP.openAjaxModal("campaign_delete",{},"Campaign.id="+campaign_id, false);
		
		TVAPP.modal.open(
			"campaign_delete",
			{"campaign_id":campaign_id},
			{context:"Campaign.id="+campaign_id}
		);
	});
	/*~-~-~-~-~-~-~-~-~- END Delete Campaigns -~-~-~-~-~-~-~-~-~*/
	
	
	
	/*~-~-~-~-~-~-~-~-~ START Jobs Summary Campaigns ~-~-~-~-~-~-~-~-~*/
	
	var handleCampJobsInitialize = function(e, data) {
		data = data || {};
		
		var modal = j$('#cboxLoadedContent');
		
		
		j$("#jCampJobsModalEdit").click(function(e){
			
			e.preventDefault();
			
			var selected_jobs = j$(".jCampJobInput:checked:not(:disabled)");
			
			var job_chex = j$(selected_jobs).valList();
			
			var num_jobs = j$(selected_jobs).length;
			
			if(num_jobs > 0) {
				j$.ajax({
					type: 'POST',
					url: TVAPP.guid('/ajax/campaigns/' + campaign_id + '/update-generic'),
					data: {'Campaign.selected_jobs_list':job_chex,'Campaign.num_jobs':num_jobs},
					success: function(result){
						j$(".jNumJobs").text(num_jobs);
						updateSummaryStatement();
						
						j$(".jCampJobInput:not(:checked)").each(function(){
							var unchecked_box = this;
							j$("#campaign_add_"+j$(unchecked_box).val()).attr("checked",false);
						});
						j$(document).trigger("updateState");
						j$(".jCloseModal",modal).trigger("click");
					},
					dataType: 'json',
					error: function(xhr, textStatus, error) { 
						TVAPP.masterErrorHandler(xhr, textStatus, error, null);
					}
				});
			} else {
				j$("body").modal({
					"text":TVAPP.smDict("campaign.error.no_jobs")
				});
			}
			
		});
		
	};
	
	j$(document).bind("campaign_summary_jobs_init", handleCampJobsInitialize);
	
	/*~-~-~-~-~-~-~-~-~ END Jobs Summary Campaigns ~-~-~-~-~-~-~-~-~*/
	
	
	/*~-~-~-~-~-~-~-~-~ START Page Summary Campaigns ~-~-~-~-~-~-~-~-~*/

	var handleCampPagesInitialize = function(e, data) {
		data = data || {};

		var modal = j$('#cboxLoadedContent');


		j$("#jCampPagesModalEdit").click(function(e){

			e.preventDefault();

			var selected_pages = j$(".jCampPageInput:checked:not(:disabled)");

			var page_chex = j$(selected_pages).valList();

			var num_pages = j$(selected_pages).length;

			if(num_pages > 0) {
				j$.ajax({
					type: 'POST',
					url: TVAPP.guid('/ajax/campaigns/' + campaign_id + '/update-generic'),
					data: {'Campaign.page_ids':page_chex,'Campaign.num_pages':num_pages},
					success: function(result){
						j$(".jNumPages").text(num_pages);
						updateSummaryStatement();
						
						j$(".jCampPageInput:not(:checked)").each(function(){
							var unchecked_box = this;
							j$("#campaign_add_"+j$(unchecked_box).val()).attr("checked",false);
						});
						j$(document).trigger("updateState");
						j$(".jCloseModal",modal).trigger("click");
					},
					dataType: 'json',
					error: function(xhr, textStatus, error) { 
						TVAPP.masterErrorHandler(xhr, textStatus, error, null);
					}
				});
			} else {
				j$("body").modal({
					"text":TVAPP.smDict("campaign.error.no_pages")
				});
			}

		});

	};

	j$(document).bind("campaign_summary_pages_init", handleCampPagesInitialize);

	/*~-~-~-~-~-~-~-~-~ END Page Summary Campaigns ~-~-~-~-~-~-~-~-~*/
	
	
	
	// Get context info from each screenshot button and open a modal to display the screenshot (stolen from info_help.js on 4/11/12 - evan)
	j$('a.jCampImgBtn').click(function(event){
		
		event.preventDefault();
		
		var mod_img_context = {
				"mod_img_path" : 'campaigns/',
				"mod_img_src" : j$(this).attr('src'),
				"mod_img_title" : TVAPP.smDict('Campaign.example_post')
		};
		
		TVAPP.openAjaxImageModal('modal_images',null,mod_img_context);
		
	});	

	// Bind Landing Page Promo modal
	j$('.jPromoLandingPages').click (function() {
		TVAPP.modal.open("landing_page_manage_modal");
	});

	var manageLandingPageModalInitialize = function(e,data) {
		data = data || {};
		var modal = j$('#cboxLoadedContent');
		j$('.jLandingPageSubmit').click(function() {
			if(j$('#jLandingPage').val() != '') {
				window.location = j$('#jLandingPage').val();
			} else {
				// open error dialog and return false
				j$('body').modal({text:TVAPP.smDict('LandingPages.Promo.submit_error')});
				return false;
			}
		});
	};
	j$(document).bind("landing_page_manage_modal_init",manageLandingPageModalInitialize);	


	
})(jQuery);


// Update Summary Statement - All Campaign Types
var updateSummaryStatement = function() {
	switch (j$('#jSummaryStatement').data('dict')) {
		case 'Recipients':
			TVAPP.pluralize('Campaign.SummaryStatement.Recipients', parseInt(j$('.jNumRecipients').text()), null, function(data) {
				j$('#jSummaryStatement').html(data.UserMessage);
			});
			break;
		case 'RecipientsAndJobs':
			TVAPP.pluralize('Campaign.SummaryStatement.RecipientsAndJobs', parseInt(j$('.jNumRecipients').text()), parseInt(j$('.jNumJobs').text()), function(data) {
				j$('#jSummaryStatement').html(data.UserMessage);
			});
			break;
		case 'RecipientsAndPages':
			TVAPP.pluralize('Campaign.SummaryStatement.RecipientsAndPages', parseInt(j$('.jNumRecipients').text()), parseInt(j$('.jNumPages').text()), function(data) {
				j$('#jSummaryStatement').html(data.UserMessage);
			});
			break;
		case 'PagesAndJobs':
			TVAPP.pluralize('Campaign.SummaryStatement.PagesAndJobs', parseInt(j$('.jNumJobs').text()), null, function(data) {
				j$('#jJobsSummary').replaceWith(data.UserMessage);
			});
			break;
		case 'PagesAndJobResults':
			TVAPP.pluralize('Campaign.SummaryStatement.PagesAndJobResults', parseInt(j$('.jNumJobs').text()), null, function(data) {
				j$('#jJobFiltersSummary').replaceWith(data.UserMessage);
			});
			break;
	}
}
