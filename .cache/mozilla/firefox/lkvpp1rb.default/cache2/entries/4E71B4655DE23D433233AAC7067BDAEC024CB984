                                    function dataTarget(name, data) {
                                        //$(".modal-body p").append(data);
                                        //$(".modal-body a img").attr("src", logo);
                                        $(".modal-header h4").empty();
                                        $(".modal-header h4").append(name);
                                        $(".modal-body div").empty();
                                        $(".modal-body div").append(data);
                                        $('#poster1').modal('show');
                                    }
                                    function updateStdProfileInEguru(fname, lname, email, mobno) {
                                        var functional = document.getElementById("functional").value;
                                        var industry = document.getElementById("industry").value;
                                        //alert(fname + " " + lname + " " + email + " " + mobno + " " + functional + " " + industry);

                                        if (fname == null || fname.trim() == "") {
                                            alert("First Name shound not be empty");
                                            return false;
                                        }
                                        if (lname == null || lname.trim() == "") {
                                            alert("Last Name shound not be empty");
                                            return false;
                                        }
                                        if (email == null || email.trim() == "") {
                                            alert("Email shound not be empty");
                                            return false;
                                        }
                                        if (!(/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/).test(email)) {
                                            alert("Invalid Email id");
                                            return false;
                                        }
                                        if (mobno == null || mobno.trim() == "") {
                                            alert("Mobile Number shound not be empty");
                                            return false;
                                        }
                                        if (industry == "") {
                                            alert("Please select industry type");
                                            return false;
                                        }
                                        if (functional == '') {
                                            alert("Please select functional type");
                                            return false;
                                        }

                                        //document.getElementById('industry').value = industry;
                                        //document.getElementById('functional').value = functional;
                                        $("#datadismisss").click();
                                        document.frm1.action = "/registerWithEguru.action";
                                        // document.frm1.setAttribute("target", "_blank");
                                        document.frm1.submit();



                                        /*      
