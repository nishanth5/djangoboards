<script>
    hideFirst();
    function hideFirst() {
        var docs = document.getElementById('moreStudentApps');
        if (docs === null) {
            hideElement('moreAdminApps');
        } else {
            hideElement('moreStudentApps');
        }
    }
    function hideElement(element) {
        var apps1 = document.getElementById(element);
        apps1.style.display = 'none';
        document.getElementById(element + 'Image').innerHTML = '<em>More</em><span ><div class="arrow"></div></span>';    // altered by Manjula          

    }

    function showMoreJobs(element) {
        var apps = document.getElementById(element);
        var displaySetting = apps.style.display;
        if (displaySetting === 'block') {
            hideElement(element);
        } else {
            apps.style.display = 'block';
            apps.style.marginTop = "5%";
            document.getElementById(element + 'Image').innerHTML = '<em>Less</em><span ><div class="arrow1"></div></span>';  // altered by Manjula          
        }
    }
</script>

<script>
    function hideOnClick() {
    
        document.body.innerHTML = '<div class="loader"></div>' + document.body.innerHTML;
    
    }
</script>

                        
