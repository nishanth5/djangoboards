<script type="text/javascript">
    // to get All jobs at a time 
    function viewAllJobs() {
        document.form1.action = "/viewStudentAllJobs.action?jobFetchLimit=0&isViewAllJobs=true";
        document.form1.submit();
        return true;        
    }
</script>


 <script type="text/javascript">
        function searchingCompanyName()
            {                
                var searchCompanyNameWithDot = "";
                document.getElementById("searchCompanyList").style.display = 'block';
                var searchCompany = document.getElementById("companyName").value;
               // var studentYear = document.getElementById("studentYear").value;
                var searchCompanyTrim = searchCompany.trim();
                var quotesNotAllowed = /^[^'"]*$/;
                if (!quotesNotAllowed.test(searchCompanyTrim)) {
                    alert("Quotes are not allowed");
                    return false;
                }

                if (searchCompanyTrim == "") {
                    document.getElementById("searchCompanyList").style.display = 'none';
                    return false;
                }
                var searchCompanyNameWithDot;
                if (searchCompanyTrim == "")
                {
                    document.getElementById("companyName").value = "";
                    return false;
                }

                var newdotstr = searchCompanyTrim;
                var newdotwords = newdotstr.split(".");
                if (newdotwords.length > 1)
                {
                    var k = 0;
                    var array1 = Array();
                    for (k = 0; k < newdotwords.length; k++)
                    {
                        if (k < newdotwords.length - 1) {
                            newdotwords[k] = newdotwords[k].trim();
                            temp = newdotwords[k].toUpperCase();
                            array1.push(temp);
                        } else {
                            temp = newdotwords[k].trim();
                            temp = temp.charAt(0).toUpperCase() + temp.substring(1);
                            array1.push(temp);
                        }
                    }
                    searchCompanyNameWithDot = array1.join(" ");
                } else {
                    searchCompanyNameWithDot = " ";
                }

                if (searchCompany != '') {
                    var url = rootdomain + "/getCollegeCompanyList.action?searchByCompany=" + searchCompanyTrim + "&searchCompanyName=" + searchCompanyNameWithDot ;
                    ajaxpageForSearchCompany(url);
                }
            }

            function ajaxpageForSearchCompany(url) {
                var page_request = false
                //if (window.XMLHttpRequest) // if Mozilla, Safari etc
                if (window.XMLHttpRequest && !document.all)
                    page_request = new XMLHttpRequest()
                else if (window.ActiveXObject) { // if IE
                    try {
                        page_request = new ActiveXObject("Msxml2.XMLHTTP")
                    } catch (e) {
                        try {
                            page_request = new ActiveXObject("Microsoft.XMLHTTP")
                        } catch (e) {
                        }
                    }
                }
                page_request.onreadystatechange = function () {
                    loadpageForSearchCompany(page_request)
                }
                if (bustcachevar) //if bust caching of external page
                    bustcacheparameter = (url.indexOf("?") != -1) ? "&" + new Date().getTime() : "?" + new Date().getTime()
                page_request.open('GET', url + bustcacheparameter, true)
                page_request.send(null)
            }

            function loadpageForSearchCompany(page_request)
            {
                if (page_request.readyState == 4 && (page_request.status == 200 || window.location.href.indexOf("http") == -1))
                    //alert(page_request.responseText);
                    if (page_request.responseText != 0)
                    {
                        document.getElementById("searchCompanyList").innerHTML = page_request.responseText;
                    }
            }

             function getCompanyNameInTextBox()
            {
                var list = document.getElementById("companyNameSearched");
                if (list.value != "" && list.value != null)
                {
                    //if u need listkey value
                    var listKey = list.options[list.selectedIndex].value;
                    document.getElementById("companyId").value = listKey;
                    //if you need listValue value
                    var listValue = list.options[list.selectedIndex].text;
                    document.getElementById("companyName").value = listValue;
                    document.getElementById("searchCompanyList").style.display = 'none';                    
                    showSearchJobList();
                } else {
                    document.getElementById("companyId").value = "";
                    //document.getElementById("collegeName").value = "";
                }
            }

            </script>
            
            <script type="text/javascript">
            function showSearchJobList() {
                var companyId = document.getElementById("companyId").value;
                $.ajax({
                    type: "POST",
                    url: rootdomain + "/showSearchJobList.action?companyId="+companyId,
                    cache: false,
                    success: function(data) { // the result jsp is come through the variable 'data', so we assign that jsp to the below id
                        $("#StudentJobListId").html(data); // we replace the jsp in this id
                    },
                    error: function() {
                        alert("Error in Getting More Jobs *** ");
                    }
                });
            }
