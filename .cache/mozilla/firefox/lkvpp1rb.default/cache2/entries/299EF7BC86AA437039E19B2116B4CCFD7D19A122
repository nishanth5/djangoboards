    <script type="text/javascript">
        /***********************************************
         * Dynamic Ajax Content- © Dynamic Drive DHTML code library (www.dynamicdrive.com)
         * This notice MUST stay intact for legal use
         * Visit Dynamic Drive at http://www.dynamicdrive.com/ for full source code
         ***********************************************/
        var bustcachevar = 1 //bust potential caching of external pages after initial request? (1=yes, 0=no)
        var loadedobjects = ""
        var rootdomain = "http://" + window.location.hostname
        var bustcacheparameter = ""

        function ajaxSearchMemberpage(url) {
            var page_request = false
            //if (window.XMLHttpRequest) // if Mozilla, Safari etc
            if (window.XMLHttpRequest && !document.all)
                page_request = new XMLHttpRequest()
            else if (window.ActiveXObject) { // if IE
                try {
                    page_request = new ActiveXObject("Msxml2.XMLHTTP")
                } catch (e) {
                    try {
                        page_request = new ActiveXObject("Microsoft.XMLHTTP")
                    } catch (e) {
                    }
                }
            }
            if (bustcachevar) //if bust caching of external page
                bustcacheparameter = (url.indexOf("?") != -1) ? "&" + new Date().getTime() : "?" + new Date().getTime()
            page_request.open('GET', url + bustcacheparameter, true)
            page_request.send(null)
            page_request.onreadystatechange = function () {
                loadpageHeader(page_request)
            }
        }

        function loadpageHeader(page_request) {
            if (page_request.readyState == 4 && (page_request.status == 200 || window.location.href.indexOf("http") == -1)) {
                document.getElementById("showSearchList").style.display = 'block';
                document.getElementById("showSearchList").innerHTML = page_request.responseText;
            }
        }
        $('html').click(function () {
            $("#showSearchList").hide();
        });
        $('#searchValueByUser').click(function (event) {
            event.stopPropagation();
        });
        $(document).on('keydown', function (e) {
            if (e.keyCode === 27) { // ESC
                $("#showSearchList").hide();
            }
        });
    </script>

    <script type="text/javascript">
        var bustcachevar = 1 //bust potential caching of external pages after initial request? (1=yes, 0=no)
        var loadedobjects = ""
        var rootdomain;
        if (window.location.hostname == 'localhost') {
            rootdomain = window.location.protocol + "//" + window.location.host;
            // It gives rootdomain = http://localhost:8080
        } else {
            rootdomain = window.location.protocol + "//" + window.location.hostname;
            // It gives rootdomain = http://localhost
        }
        var bustcacheparameter = ""
        function  ajaxSeeAllResultspage(url) {
            var page_request = false
            //if (window.XMLHttpRequest) // if Mozilla, Safari etc
            if (window.XMLHttpRequest && !document.all)
                page_request = new XMLHttpRequest()
            else if (window.ActiveXObject) { // if IE
                try {
                    page_request = new ActiveXObject("Msxml2.XMLHTTP")
                } catch (e) {
                    try {
                        page_request = new ActiveXObject("Microsoft.XMLHTTP")
                    } catch (e) {
                    }
                }
            }
            if (bustcachevar) //if bust caching of external page
                bustcacheparameter = (url.indexOf("?") != -1) ? "&" + new Date().getTime() : "?" + new Date().getTime()
            page_request.open('GET', url + bustcacheparameter, true)
            page_request.send(null)
            page_request.onreadystatechange = function () {
                loadViewpageHeader(page_request)
            }
        }

        function loadViewpageHeader(page_request) {
            if (page_request.readyState == 4 && (page_request.status == 200 || window.location.href.indexOf("http") == -1)) {
                document.getElementById("textAreaMessage").style.display = 'none';
                document.getElementById("divShowMessageBoard").style.display = 'none';
                document.getElementById("SeeMoreResults").style.display = 'block';
                document.getElementById("SeeMoreResults").innerHTML = page_request.responseText;
            }
        }
        function viewAllSearchResults(value) {
            document.logoutForm.action = "/doMemberSearchResult.action?searchValueByUser=" + value;
            document.logoutForm.submit();
            return true;
        }
        function viewAllSearch() {
            var value = document.getElementById("searchValueByUser").value;
            document.logoutForm.action = "/doMemberSearchResult.action?searchValueByUser=" + value;
            document.logoutForm.submit();
            return true;
        }
    </script>
    <script>
        function callSearchMethod(searchValue, keyCode) {
            searchValue = searchValue.toLowerCase();
            searchValue = searchValue.replace(/[%\"!`$%'+@,-_{}()^~*?:\#< >()|]/g, " ");
            if (!(keyCode == 37 || keyCode == 39 || keyCode == 27))
            {
                if (searchValue != null && searchValue != '') {
                    if (searchValue.length > 100) {
                        alert("Only " + 100 + " characters are allowed");
                    }
                    ajaxSearchMemberpage(rootdomain + "/doMemberSearchList.action?searchValueByUser=" + searchValue + "&requestFromAjax=true")
                } else {
                    document.getElementById("showSearchList").style.display = 'none';
                    return false;
                }
            }
        }

        function showSearchResult() {
            var value = document.getElementById("searchValueByUser").value;
            value = value.toLowerCase();
            value = value.replace(/[%\"!`$%'+@,-_{}()^~*?:\#< >()|]/g, " ");
            if (value != null && value != '') {
                ajaxSeeAllResultspage(rootdomain + "/doMemberSearchResult.action?searchValueByUser=" + value);
                return true;
            } else {
                return false;
            }
        }
    </script>

    

    <script type="text/javascript">
        function dologout() {
            delete_Cookie('cookuser', '/', '');
            document.logoutForm.action = "/doLogOut.action";
            document.logoutForm.submit();
        }

        function delete_Cookie(name, path, domain) {
            set_Cookie('cookuser', '', 'Thu, 01-Jan-1970 00:00:01 GMT', '/', '', '');
            set_Cookie('cookpass', '', 'Thu, 01-Jan-1970 00:00:01 GMT', '/', '', '')
        }

        function set_Cookie(name, value, expires, path, domain, secure)
        {
            // set time, it's in milliseconds
            var today = new Date();
            today.setTime(today.getTime());
            if (expires)
            {
                expires = expires * 1000 * 60 * 60 * 24;
            }
            var expires_date = new Date(today.getTime() + (expires));

            document.cookie = name + "=" + escape(value) +
                    ((expires) ? ";expires=" + expires_date.toGMTString() : "");
            ((path) ? ";path=" + path : "") +
                    ((domain) ? ";domain=" + domain : "") +
                    ((secure) ? ";secure" : "");
        }
    </script>

    

    <script type='text/javascript'>
        /* Script to Close the left menu onclick of the menu Icon */
        var clicks = 0;
        $(window).load(function () {

            $(".myButton").click(function () {

                // Set the effect type
                var effect = 'slide';

                // Set the options for the effect type chosen
                var options = {direction: 'up'};

                // Set the duration (default: 400 milliseconds)
                var duration = 'slow';

                $('.myDiv').toggle(effect, options, duration);

                if (clicks == 0) {
                    clicks++;
                    $('.dashboarddisplay').css({'height': $('.leftpanel').height()});
                    //Script lines to reduce its height to 500px 
                    //           var leftpanelHeight=$('.leftpanel').height()+'px'.toString();
                    //            $('.dashboarddisplay').css('height',leftpanelHeight);
                    //            alert(leftpanelHeight);
                    //Script lines for reduced height but unable to reduce html body height 
                    //$('.leftpanel').css({'height':  $( window ).height()});
                    //$('.dashboarddisplay').css({'height':  $( window ).height()});
                    //alert($( window ).height());
                    //alert($('.dashboarddisplay').height());
                    $("html, body").animate({scrollTop: 0}, 600);
                } else {
                    $('.dashboarddisplay').css({'height': 'auto'});
                    clicks--;
                }
            });
        });
    </script>
