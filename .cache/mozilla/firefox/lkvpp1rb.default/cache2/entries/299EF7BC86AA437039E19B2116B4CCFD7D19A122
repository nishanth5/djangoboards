//here '$s' is replaced in place of '$', because of conflict between js. By change the variable, this script never conflict with others.
            $(function ($s) {
                $s('.ui.accordion')
                        .accordion()
                        ;
                $s('.ui.dropdown')
                        .dropdown()
                        ;
                $s('.ui.radio.checkbox')
                        .checkbox()
                        ;

                // Logout Dropdonw
                $('.profilelefttextlogout').click(function () {
                    $('#logoutdrp').slideToggle(300);
                });
                $(document).on('click', function (event) {
                    if ($('.profilelefttextlogout').has(event.target).length == 0) {
                        $('#logoutdrp').slideUp(300);
                    }
                });
                // Logout Dropdonw
            });
        </script>
