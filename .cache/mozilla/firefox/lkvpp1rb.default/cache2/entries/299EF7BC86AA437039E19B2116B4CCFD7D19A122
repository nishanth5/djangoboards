        function showHallTicket(jobId)
        {
            var showHallTicket = "showHallTicket" + jobId;
            document.getElementById(showHallTicket).style.display = 'none';
            var enableHallTicketDiv = "enableHallTicketDiv" + jobId;
            document.getElementById(enableHallTicketDiv).style.display = 'block';
            return false;
        }
