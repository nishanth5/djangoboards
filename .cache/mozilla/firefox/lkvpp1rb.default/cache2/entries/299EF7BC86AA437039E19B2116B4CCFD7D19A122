<script type="text/javascript">// script to call the more jobs while scroll down 
    $(window).scroll(function() {
        if ($(document).height() <= $(window).scrollTop() + $(window).height()) {
            if (true) {
                //design for loading at bottom
                var code = "<img style='margin:-4px auto;' src='https://campusinteraction.com/Campus/ci-website//moreajaximageblue.gif' />";
                document.getElementById("moreJob").innerHTML = code;
                showMore();
            }
        }
    });
</script>
<script type="text/javascript">
    function showMore() {
        var jobFetchLimit = document.getElementById("jobFetchLimit").value;
        $.ajax({
            type: "POST",
            url: rootdomain + "/showStudentMoreJobList.action?jobFetchLimit=" + jobFetchLimit,
            cache: false,
            success: function(data) { // the result jsp is come through the variable 'data', so we assign that jsp to the below id
                $("#StudentJobListId").html(data); // we replace the jsp in this id
            },
            error: function() {
                alert("Error in Getting More Jobs *** ");
            }
        });
    }
</script>
