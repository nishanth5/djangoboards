    var bustcacheparameter = ""

    function loginInToFBForPlayEnglishGame() {
        ajaxpageEntryInAuditLogin(rootdomain + "/auditForGame.action")

        FB.getLoginStatus(function (response) {
            if (response.status === "connected") {
                document.logoutForm.action = "/getQuestionsForOnlineTestGame.action?type=English";
                document.logoutForm.submit();
                return true;
            } else {
                document.logoutForm.action = "/gameInstrution.action";
                document.logoutForm.submit();
                return true;
            }
        });
    }
</script>
<script type="text/javascript">
    function ajaxpageEntryInAuditLogin(url) {
        var page_request = false
        if (window.XMLHttpRequest && !document.all)
            page_request = new XMLHttpRequest()
        else if (window.ActiveXObject) {
            try {
                page_request = new ActiveXObject("Msxml2.XMLHTTP")
            } catch (e) {
                try {
                    page_request = new ActiveXObject("Microsoft.XMLHTTP")
                } catch (e) {
                }
            }
        }
        page_request.onreadystatechange = function () {
            loadpageEntryInAuditLogin(page_request)
        }
        if (bustcachevar)
            bustcacheparameter = (url.indexOf("?") != -1) ? "&" + new Date().getTime() : "?" + new Date().getTime()
        page_request.open('GET', url + bustcacheparameter, true)
        page_request.send(null)
    }

    function loadpageEntryInAuditLogin(page_request) {
        if (page_request.readyState == 4 && (page_request.status == 200 || window.location.href.indexOf("http") == -1))
            var pageResponse = page_request.responseText.trim();
        if (pageResponse == 'InvalidSessin') {
            window.open("http://www.campusinteraction.com/", "_self")
        } else {

        }
    }
</script>    
