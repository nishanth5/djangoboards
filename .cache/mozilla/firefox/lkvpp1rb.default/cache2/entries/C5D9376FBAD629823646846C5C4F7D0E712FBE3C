<script type="text/javascript">

    $(document).ready(function() {

        var currentDate = new Date();
        var closingDate = document.getElementById('closingDate').value;
        var closingTime = document.getElementById('closingTime').value;
        if (closingTime != null && closingTime != "") {
            var delimit = ":";
            closingTimeArray = closingTime.split(delimit);
            for (var i = 0; i < closingTimeArray.length; i++) {
            }
            if (closingTimeArray[1] == "00") {
                var time = ((Number(closingTimeArray[0])) * 1000 * 60 * 60);
            } else if (closingTimeArray[1] == "15") {
                var time = (Number(closingTimeArray[0]) + Number(0.25)) * 1000 * 60 * 60;
            } else if (closingTimeArray[1] == "30") {
                var time = (Number(closingTimeArray[0]) + Number(0.5)) * 1000 * 60 * 60;
            } else if (closingTimeArray[1] == "45") {
                var time = (Number(closingTimeArray[0]) + Number(0.75)) * 1000 * 60 * 60;
            }
        }
        else {
            var time = 0;

        }

        if (closingDate != null && closingDate != "") {
            var delimiter = "-";
            campusOnTimeArray = closingDate.split(delimiter);
            for (var i = 0; i < campusOnTimeArray.length; i++) {
            }
            var modifyDate = campusOnTimeArray[1] + "/" + campusOnTimeArray[0] + "/" + campusOnTimeArray[2];
            var closeDate = new Date(modifyDate);
            var closeDate = new Date(closeDate.getTime() + time);
            if (closeDate >= currentDate) {
                document.getElementById('dateExpired').style.display = 'block';
                //Added this dateExpired1 id since included accept and decline in top and bottom of the page
                document.getElementById('dateExpired1').style.display = 'block';
            }
            else {
                document.getElementById('dateExpired').style.display = 'none';
                //Added this dateExpired1 id since included accept and decline in top and bottom of the page
                document.getElementById('dateExpired1').style.display = 'none';
            }
        }
        else {
            document.getElementById('dateExpired').style.display = 'block';
            //Added this dateExpired1 id since included accept and decline in top and bottom of the page
            document.getElementById('dateExpired1').style.display = 'block';
        }
    });


    function acceptJob(action, blockedStudent)
    {
        if (blockedStudent == 'Yes') {
            alert("You are not eligible to apply this job. \n Please contact the TAP CELL for any clarification.")
            return false
        } else {
            document.getElementById('JobId').value = action;
            document.frm1.action = "/applyJobAction.action";
            document.frm1.submit();
        }
        return true;

    }

    function declineJob(action)
    {
        document.getElementById('JobId').value = action;
        document.frm1.action = "/declineJobAction.action";
        document.frm1.submit();
        return true;
    }
    function alumniJob(action)
    {
        document.getElementById('JobId').value = action;
        document.frm1.action = "/applyAlumniJob.action";
        document.frm1.submit();
        return true;
    }
    function filterCandidates(action)
    {
        document.getElementById('pageFrom').value = "selectedStudentList";
        document.getElementById('JobId').value = action;
        document.frm1.action = "/initialFilterStudent.action";
        document.frm1.submit();
        return true;
    }

    function addBranchToFilterCandidates(action)
    {
        document.getElementById('pageFrom').value = "updateFilterCriteria";
        document.getElementById('JobId').value = action;
        document.getElementById('addBranch').value = true;
        document.frm1.action = "/initialFilterStudent.action";
        document.frm1.submit();
        return true;
    }

    function addCandidate(action)
    {
        document.getElementById('JobId').value = action;
        document.frm1.action = "/showAddCandidate.action";
        document.frm1.submit();
        return true;
    }

    //This method used to View the Job details for Update using JobID
    function viewJob(action) {
        document.getElementById('JobId').value = action;
        document.frm1.action = "/viewJobDetails.action";
        document.frm1.submit();
        return true;

    }
    //This method used to View the Compnay details for Update using JobID
    function viewCompany(action) {
        document.getElementById('companyId').value = action;
        document.frm1.action = "/viewCompanyDetails.action";
        document.frm1.submit();
        return true;

    }

    function deleteJob(JobId)
    {
        if (confirm("Are you sure want to delete?")) {
            document.getElementById('JobId').value = JobId;
            document.frm1.action = "/doDeleteJob.action";
            document.frm1.submit();

        }
    }

    function uploadSelectedCandidate(action, stuYear, status)
    {
        document.getElementById('JobId').value = action;
        document.getElementById('studentYear').value = stuYear;
        document.getElementById('status').value = status;
        document.frm1.action = "/showSelectedStudent.action";
        document.frm1.submit();
        return true;
    }

    function uploadShortListCandidate(action, stuYear, status)
    {
        document.getElementById('JobId').value = action;
        document.getElementById('studentYear').value = stuYear;
        document.getElementById('status').value = status;
        document.frm1.action = "/showSelectedStudent.action";
        document.frm1.submit();
        return true;
    }
    
    function uploadInterviewRound(action, stuYear)
    {
        document.getElementById('JobId').value = action;
        document.getElementById('studentYear').value = stuYear;
        document.frm1.action = "/showUploadInterviewRound.action";
        document.frm1.submit();
        return true;
    }

</script>
<script type="text/javascript">
    window.fbAsyncInit = function() {
        FB.init({appId: '449344988425871', status: true, cookie: true, xfbml: true});

        /* All the events registered */
        FB.Event.subscribe('auth.login', function(response) {
            // do something with response
            login();
        });
        FB.Event.subscribe('auth.logout', function(response) {
            // do something with response
            logout();
        });

        FB.getLoginStatus(function(response) {
            if (response.session) {
                // logged in and connected user, someone you know
                login();
                FB.login(function(response) {
                    // handle the response
                }, {scope: 'email,user_birthday,user_location,user_hometown,user_education_history,status_update,publish_stream'});
            }
        });
        FB.Canvas.setSize({width: 940, height: 480});



    };
    (function() {
        var e = document.createElement('script');
        e.type = 'text/javascript';
        e.src = document.location.protocol +
                '//connect.facebook.net/en_US/all.js';
        e.async = true;
        document.getElementById('fb-root').appendChild(e);
    }());

    function login() {
        FB.api('/me', function(response) {
            // document.getElementById('login').style.display = "block";
        });
    }
    function logout() {

    }

